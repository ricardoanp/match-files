{
  "name": "match-backend",
  "version": "1.0.0",
  "description": "Match! API - Marketplace de quadras e eventos esportivos",
  "main": "src/server.ts",
  "type": "module",
  "scripts": {
    "dev": "NODE_ENV=development nodemon --exec ts-node src/server.ts",
    "build": "tsc",
    "start": "NODE_ENV=production node dist/server.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "migrate": "npx knex migrate:latest",
    "seed": "npx knex seed:run",
    "typecheck": "tsc --noEmit"
  },
  "keywords": ["match", "sports", "courts", "booking"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "express-rate-limit": "^7.0.0",
    "jsonwebtoken": "^9.1.2",
    "pg": "^8.11.3",
    "knex": "^3.1.0",
    "uuid": "^9.0.1",
    "argon2": "^0.31.2",
    "dotenv": "^16.3.1",
    "joi": "^17.11.0",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "winston": "^3.11.0",
    "stripe": "^14.7.0",
    "bull": "^4.11.5",
    "ioredis": "^5.3.2",
    "pino": "^8.16.2",
    "pino-http": "^8.6.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.5",
    "@types/jest": "^29.5.11",
    "typescript": "^5.3.3",
    "ts-node": "^10.9.2",
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "supertest": "^6.3.3"
  }
}
